{
  "name": "xlang",
  "displayName": "xlang",
  "description": "X language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "x",
      "aliases": ["X", "x"],
      "extensions": [".x",".xs",".xd"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "x",
      "scopeName": "source.x",
      "path": "./syntaxes/x.tmLanguage.json"
    }]
  }
}
